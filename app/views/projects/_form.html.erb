<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
  </div>
  <div class="form-inputs">
    <%= f.input :description %>
  </div>
  <div class="form-inputs">
    <%= f.input :organization_id, collection: @organizations %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
